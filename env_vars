#!/bin/bash
# ; -*- mode: shell-script;-*-
# set helper environment

# qemu inside container
export Q_P=/usr/local/bin/qemu-system-x86_64
export Q_TOP=/home/qemu.git

# current container image on host
# docker_c: image create
# docker_u: update image from running container
export D_IMG=dockerqemu:latest

# work dir mapping in buildlib.sh:docker_r
export D_WORK=/home/work

echo "setting..."
printenv | egrep "^Q_|^D_"


